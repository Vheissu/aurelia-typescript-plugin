{
  "name": "aurelia-typescript-plugin",
  "version": "1.0.0",
  "description": "TypeScript based plugin structure for Aurelia plugins.",
  "keywords": [
    "skeleton",
    "plugin",
    "aurelia"
  ],
  "main": "dist/commonjs/index.js",
  "typings": "dist/commonjs/index.d.ts",
  "scripts": {
    "lint": "cross-env tslint --project tsconfig.json",
    "pretest": "cross-env npm run lint",
    "test": "cross-env tsc && karma start --single-run",
    "test-watch": "concurrently \"./node_modules/.bin/tsc --watch\" \"./node_modules/.bin/karma start\"",
    "build:amd": "cross-env tsc --project tsconfig.build.json --outDir dist/amd --module amd",
    "build:commonjs": "cross-env tsc --project tsconfig.build.json --outDir dist/commonjs --module commonjs",
    "build:es2015": "cross-env tsc --project tsconfig.build.json --outDir dist/es2015 --module es2015",
    "build:native-modules": "cross-env tsc --project tsconfig.build.json --outDir dist/native-modules --module es2015",
    "build:system": "cross-env tsc --project tsconfig.build.json --outDir dist/system --module system",
    "prebuild": "cross-env rimraf dist",
    "postbuild": "gulp",
    "build": "concurrently \"npm run build:amd\" \"npm run build:commonjs\" \"npm run build:es2015\" \"npm run build:native-modules\" \"npm run build:system\"",
    "prepare-release": "cross-env npm run build && semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vheissu/aurelia-typescript-plugin.git"
  },
  "author": "Dwayne Charrington <dwaynecharrington@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/Vheissu/aurelia-typescript-plugin#readme",
  "bugs": {
    "url": "https://github.com/Vheissu/aurelia-typescript-plugin/issues"
  },
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "index",
    "format": "cjs",
    "directories": {
      "dist": "dist/commonjs"
    }
  },
  "dependencies": {
    "aurelia-binding": "^1.2.1",
    "aurelia-dependency-injection": "^1.3.1",
    "aurelia-event-aggregator": "^1.0.1",
    "aurelia-framework": "^1.1.2",
    "aurelia-loader": "^1.0.0",
    "aurelia-logging": "^1.3.1",
    "aurelia-metadata": "^1.0.3",
    "aurelia-pal": "^1.3.0",
    "aurelia-path": "^1.1.1",
    "aurelia-task-queue": "^1.2.0",
    "aurelia-templating": "^1.4.2",
    "aurelia-templating-binding": "^1.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.52",
    "@types/node": "^8.0.0",
    "aurelia-bootstrapper": "^2.1.1",
    "aurelia-pal-browser": "^1.2.1",
    "aurelia-polyfills": "^1.2.1",
    "aurelia-testing": "^1.0.0-beta.3.0.1",
    "autoprefixer": "^7.1.1",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.1",
    "gulp": "^3.9.1",
    "gulp-sass": "^3.1.0",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-requirejs": "^1.1.0",
    "node-sass": "^4.5.3",
    "requirejs": "^2.3.3",
    "requirejs-text": "^2.0.15",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.6",
    "tslint": "^5.4.3",
    "typescript": "rc"
  }
}
